{
    "name": "BrianTweed/percolator",
    "description": "Laravel package to automatically build and apply filter queries from existing model scopes",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Tweed",
            "email": "brtweed@outlook.com"
        }
    ],
    "homepage": "https://github.com/briantweed/percolator",
    "keywords": ["Laravel"],
    "require": {
        "illuminate/support": "~5"
    },
    "autoload": {
        "psr-4": {
            "BrianTweed\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrianTweed\\PercolatorServiceProvider"
            ],
            "aliases": {
                "Percolator": "BrianTweed\\Facades\\Percolator"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}